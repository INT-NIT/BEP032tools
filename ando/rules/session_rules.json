{
  "session_level": {
    "regexp": "^ses-(?P<date>\\d{8})_(?P<sesNumber>\\d{3})_(?P<customSesField>[0-9a-zA-Z-]*\\Z)"

  },
  "sessions_files_ses_level": {
    "regexp": "^(exp-[a-zA-Z0-9]+)[\\/\\\\](sub-[a-zA-Z0-9]+)[\\/\\\\]ses-(\\d{8})_(\\d{3})((_[0-9a-zA-Z-]+)??)[\\/\\\\]ses-(\\d{8})_(\\d{3})((_[0-9a-zA-Z-]+)??)(@@@_ses_level_ext_@@@)$",
    "tokens": {
      "@@@_ses_level_ext_@@@": [
        "\\.tsv",
        "\\.json"
      ]
    }
  },
  "task_files": {
      "regexp": "(exp-[a-zA-Z0-9]+)[\\/\\\\](sub-[a-zA-Z0-9]+)[\\/\\\\]ses-(\\d{8})_(\\d{3})((_[0-9a-zA-Z-]+)??)[\\/\\\\](sub-[a-zA-Z0-9]+)_(ses-(\\d{8})_(\\d{3})((_[0-9a-zA-Z-]+)??))((_runs)??)(@@@_task_ext_@@@)$",
      "tokens": {
        "@@@_task_ext_@@@": [
          "\\.tsv",
          "\\.json"
        ]
      }
    }
}