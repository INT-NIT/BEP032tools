// new rules with optional token not used yet
{ 
    "isAndo": {
        "regexp": "^(@@@_exp_lvl_@@@)((@@@_sub_lvl_@@@))((@@@_ses_lvl_@@@)?)((@@@_in_ses_@@@)?)",
        "tokens": {
          "@@@_exp_lvl_@@@": [
          "(exp-[a-zA-Z0-9]+)[\\/\\\\]"
          ],
          "@@@_sub_lvl_@@@": [
            "(sub-[a-zA-Z0-9]+)"
            ],
          "@@@_ses_lvl_@@@": [
            "[\\/\\\\](ses-\\d{8}_\\d{3}_[a-zA-Z0-9-]+)"
            ]
          },
          "@@@_in_ses_@@@": [
            "[\\/\\\\](metadata)",
            "[\\/\\\\](rawdata)",
            "[\\/\\\\](derivatives)"
            ]
          
    }
  }